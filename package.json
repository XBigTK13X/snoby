{
  "name": "snowby",
  "version": "3.1.4",
  "description": "Emby frontend that plays media in an external player.",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "develop": "concurrently -k 'yarn:develop-sass' 'yarn:start'",
    "develop-sass": "node-sass --watch ./src/asset/sass -o ./src/asset/css",
    "package": "npm run format-all && electron-packager . --overwrite --icon=./src/asset/img/snowflake.ico --ignore pdn/ --ignore mpv.log --ignore keep-awake_data/ --ignore snowby-ipc.log",
    "format-all": "prettier --write \"src/**/*.{js,html,css,scss}\"",
    "retag-files": "node src/script/retag-files.js"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "5.2.0",
    "electron": "9.0.4",
    "electron-packager": "14.2.1",
    "node-sass": "4.14.1",
    "nodemon": "2.0.4",
    "prettier": "2.0.5"
  },
  "dependencies": {
    "axios": "0.19.2",
    "debounce": "1.2.0",
    "jquery": "3.5.1",
    "jquery-lazy": "1.7.10",
    "lodash": "4.17.15",
    "moment": "2.26.0",
    "query-string": "6.13.1",
    "tippy.js": "5.2.1"
  }
}
