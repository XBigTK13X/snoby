{
  "name": "snowby",
  "version": "2.7.2",
  "description": "Emby frontend that plays media in an external player.",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "develop": "concurrently 'yarn:develop-sass' 'yarn:start'",
    "develop-sass": "node-sass --watch ./src/asset/sass -o ./src/asset/css",
    "package": "npm run format-all && electron-packager . --overwrite --icon=./src/asset/img/snowflake.ico --ignore pdn/ --ignore mpv.log --ignore keep-awake_data/",
    "format-all": "prettier --write \"src/**/*.{js,html,css,scss}\""
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^5.0.2",
    "electron": "^7.1.6",
    "electron-packager": "^14.1.1",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "cuid": "^2.1.8",
    "debounce": "^1.2.0",
    "jquery": "^3.4.1",
    "jquery-lazy": "^1.7.10",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "query-string": "^6.9.0",
    "tippy.js": "^5.1.2"
  }
}
